<div class="container">

  <table class="table table-striped table-bordered">
    <tr>
      <th>Name</th>
      <th>Industry</th>
    </tr>

    <% @accounts.each do |account| %>
      <tr>
        <td><%= account.name %></td>
        <td><%= account.industry %></td>
      </tr>
    <% end %>
  </table>

</div>
